# Spotify API Credentials
# Get these from: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Last.fm API Key
# Get this from: https://www.last.fm/api/account/create
LASTFM_API_KEY=your_lastfm_api_key_here

# Google Gemini API Key (for AI explanations)
# Get this from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OAuth Redirect URI (for playlist creation)
# For local: http://localhost:3000/auth/callback
# For production: https://your-app.onrender.com/auth/callback
REDIRECT_URI=http://localhost:3000/auth/callback
